{
  "rewrites": [
    {
      "source": "/stats/js/script.js",
      "destination": "https://plausible.io/js/script.js"
    },
    {
      "source": "/stats/api/event",
      "destination": "https://plausible.io/api/event"
    }
  ],
  "headers": [
    {
      "source": "/about",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400, s-maxage=86400, stale-while-revalidate=59"
        }
      ]
    },
    {
      "source": "/mining",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400, s-maxage=86400, stale-while-revalidate=59"
        }
      ]
    },
    {
      "source": "/wallet",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400, s-maxage=86400, stale-while-revalidate=59"
        }
      ]
    }
  ]
}

